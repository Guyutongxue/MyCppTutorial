{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "**/*.ts",
    "**/*.vue"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@src/*": [
        "./src/*"
      ],
      "@plugins/*": [
        "./plugins/*"
      ],
    },
    "types": [
      "vite/client"
    ],
  }
}
